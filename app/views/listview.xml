<Alloy>

	<Collection src="GooglePlace"/>

	<Window id="win" title="List View">
		
		<TableView dataCollection="GooglePlace" onClick="tableClick">
		
			<TableViewRow hasDetail="true" model="{id}">
				<ImageView class="icon" image="{icon}"/>
				<Label class="nameLabel" text="{name}"/>
				<Label class="vicinityLabel" text="{vicinity}"/>
			</TableViewRow>
				
		</TableView>
		
	</Window>
	
</Alloy>
